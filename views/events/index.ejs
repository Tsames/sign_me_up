<!DOCTYPE html>
<html lang="en">
<%- include("../partials/head.ejs") %>

  <body>
    <%- include("../partials/navbar.ejs") %>

    <div class="container">
      <% for (let i=0; i < events.length; i++) { %>
        <% if (i === 0 || i % 3 === 0) { %>
          <div class="row">
        <% } %>
            <div class="col-sm-3">
              <div class="panel panel-primary">
                <div class="panel-heading"><%= events[i].name %></div>
                <div class="panel-body"><img src="<%= events[i].image %>" class="img-responsive"
                    style="width:100%" alt="Image"></div>
                <div class="panel-footer">
                  <%= events[i].date.toDateString() %><br>
                  <%= events[i].address %><br>
                  <%= events[i].city %>, <%=events[i].state %>
                </div>
              </div>
            </div>
      <% } %>
    </div>
  </body>

</html>